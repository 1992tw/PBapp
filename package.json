    "@react-navigation/stack": "^6.4.1",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.72.8",
    "@types/react-native-vector-icons": "^6.4.18",
    "typescript": "~5.3.3"
  },
  "private": true
}
