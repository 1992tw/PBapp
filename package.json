{
  "name": "pbapp",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start:dev": "npm cache clean --force && react-native start --reset-cache",
    "start:prod": "ENVFILE=.env.production expo start",
    "clean": "rimraf node_modules/ && npm cache clean --force && rm -rf package-lock.json && rm -rf .expo && rm -rf .react-native && echo 'Cleaned project!' && npm install && npm run start:dev"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/datetimepicker": "^8.2.0",
